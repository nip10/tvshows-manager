{
  "name": "tvshows-manager",
  "version": "1.0.0",
  "description": "Tvshow Manager",
  "main": "./dist/server.js",
  "scripts": {
    "build:server": "babel src -d dist --ignore src/public",
    "build:client": "gulp --production",
    "lint": "eslint .",
    "start": "node ./dist/server.js",
    "watch:assets": "gulp serve",
    "watch:server": "nodemon ./src/server.js --exec babel-node",
    "watch:debug": "nodemon ./src/server.js --exec babel-node --inspect",
    "migrate": "knex migrate:rollback --knexfile ./src/db/knexfile.js --env production && knex migrate:latest --knexfile ./src/db/knexfile.js --env production",
    "seed": "knex seed:run --knexfile ./src/db/knexfile.js --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nip10/tvshows-manager.git"
  },
  "author": "Diogo Cardoso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nip10/tvshows-manager/issues"
  },
  "homepage": "https://github.com/nip10/tvshows-manager#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cron": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "knex": "^0.14.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.4.5",
    "uuid": "^3.1.0",
    "validator": "^9.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.5.0",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.4.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.0.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.4",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "lazypipe": "^1.0.1",
    "nodemon": "^1.12.5",
    "postcss-bem": "^0.4.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.0.0",
    "regenerator-runtime": "^0.11.0",
    "run-sequence": "^2.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  }
}
