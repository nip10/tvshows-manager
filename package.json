{
  "name": "tvshows-manager",
  "version": "1.0.0",
  "description": "TvShow Manager build with Node/Express/Postgres",
  "main": "server/server.js",
  "scripts": {
    "build": "gulp --production",
    "lint": "eslint .",
    "start": "node server/server.js --exec babel-node",
    "start:remote": "set NODE_ENV=production&&node server/server.js --exec babel-node",
    "watch:assets": "gulp serve",
    "watch:server": "nodemon server/server.js --exec babel-node",
    "watch:debug": "nodemon server/server.js --exec babel-node --inspect",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nip10/tvshows-manager.git"
  },
  "author": "Diogo Cardoso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nip10/tvshows-manager/issues"
  },
  "homepage": "https://github.com/nip10/tvshows-manager#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.0",
    "cookie-parser": "^1.4.3",
    "cron": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "jdenticon": "^1.7.2",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "pug": "^2.0.0-rc.4",
    "randomstring": "^1.1.5",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.4.3",
    "uuid": "^3.1.0",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "del": "^3.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.8.0",
    "gulp-eslint": "^4.0.0",
