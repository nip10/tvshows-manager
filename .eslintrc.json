{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "indent": ["error", 4]
  },
  "overrides": [{
    "files": ["src/js/app.js"],
    "parserOptions": {
      "sourceType": "script"
    },
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "Chart": true,
      "moment": true,
      "Bloodhound": true
    }
  }]
}